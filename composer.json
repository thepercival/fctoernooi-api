{
	"name": "slim/slim-skeleton",
	"description": "A Slim Framework skeleton application for rapid development",
	"keywords": ["sport toernooi"],
	"homepage": "http://github.com/slimphp/Slim-Skeleton",
	"license": "MIT",
	"authors": [
        	{
            		"name": "Josh Lockhart",
            		"email": "info@joshlockhart.com",
            		"homepage": "http://www.joshlockhart.com/"
        	}
    	],
	"require": {
		"thepercival/voetbal": "dev-master",
		"php": "^7.2",
		"slim/slim": "^3.1",
		"slim/php-view": "^2.0",
		"monolog/monolog": "^1.24",
		"doctrine/orm": "^2.6",
		"tuupola/base62": "^0.11.0",
		"tuupola/cors-middleware": "^0.9.0",
		"tuupola/slim-basic-auth": "^3.2",
		"tuupola/slim-jwt-auth": "^3.1",
		"crell/api-problem": "^3.0",
		"gofabian/negotiation-middleware": "^0.1.2",
		"vlucas/phpdotenv": "^2.5",
		"symfony/property-access": "^4.2",
		"phpmailer/phpmailer": "^6.0",
		"jms/serializer": "^2.0",
		"symfony/yaml": "^4.2",
		"zf1/zend-pdf": "^1.12"
    	},
	"require-dev": {
		"phpunit/phpunit": "^7.0"
	},
	"autoload": {
		"psr-4": {
			"FCToernooi\\": "domain/",
			"App\\Action\\": "app/actions",
			"App\\Middleware\\": "app/middleware",
			"App\\Response\\": "app/response"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"FCToernooi\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"start": "php -S localhost:2999 -t public public/index.php",
		"test": [
			"vendor/bin/phpunit --bootstrap vendor/autoload.php tests/cases/"
		]
	}
}
