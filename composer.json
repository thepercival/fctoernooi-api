{
	"name": "slim/slim-skeleton",
	"description": "A Slim Framework skeleton application for rapid development",
	"keywords": ["microframework", "rest", "router", "psr7"],
	"homepage": "http://github.com/slimphp/Slim-Skeleton",
	"license": "MIT",
	"authors": [
        	{
            		"name": "Josh Lockhart",
            		"email": "info@joshlockhart.com",
            		"homepage": "http://www.joshlockhart.com/"
        	}
    	],
	"require": {
		"thepercival/voetbal": "dev-master",
		"php": "^7.2",
		"slim/slim": "^3.1",
		"slim/php-view": "^2.0",
		"monolog/monolog": "^1.17",
		"doctrine/orm": "^2.5",
		"tuupola/base62": "^0.9.0",
		"tuupola/cors-middleware": "^0.7.0",
		"tuupola/slim-basic-auth": "^3.0",
		"tuupola/slim-jwt-auth": "^3.0",
		"crell/api-problem": "^2.0",
		"gofabian/negotiation-middleware": "^0.1.2",
		"vlucas/phpdotenv": "^2.4",
		"symfony/property-access": "^3.1",
		"phpmailer/phpmailer": "^5.2",
		"jms/serializer": "^1.5",
		"symfony/yaml": "^3.2",
		"zf1/zend-pdf": "^1.12"
    	},
	"require-dev": {
		"phpunit/phpunit": "6.x"
	},
	"autoload": {
		"psr-4": {
			"FCToernooi\\": "domain/",
			"App\\Action\\": "app/actions",
			"App\\Middleware\\": "app/middleware",
			"App\\Response\\": "app/response"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"FCToernooi\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"start": "php -S localhost:2999 -t public public/index.php",
		"test": [
			"vendor/bin/phpunit --bootstrap vendor/autoload.php tests/"
		]
	}
}
